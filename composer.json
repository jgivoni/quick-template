{
    "name": "jgivoni/quick-template",
    "description": "A starting point and consolidated structure",
    "keywords": [
        "jgivoni",
        "template"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jakob Givoni",
            "email": "jakob@givoni.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "jgivoni\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    },
    "require": {
        "php": "^8.3",
        "ext-mbstring": "*",
        "ext-json": "*",
        "ext-zend-opcache": "*",
        "ext-intl": "*"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "friendsofphp/php-cs-fixer": "^3.52",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^11.0"
    }
}
